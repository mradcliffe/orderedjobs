<?xml version="1.0" encoding="UTF-8" ?>
<phpunit
  bootstrap="tests/bootstrap.php"
  verbose="true"
>
    <testsuites>
        <testsuite name="Ordered Jobs Test Suite">
            <directory>tests</directory>
            <exclude>./vendor</exclude>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory>src</directory>
        </whitelist>
    </filter>
</phpunit>